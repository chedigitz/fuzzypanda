.block
  .secondary-navigation
    %ul.wat-cf
      %li.first=link_to pat(:list), url(:streamhubs, :index)
      %li.active=link_to pat(:new), url(:streamhubs, :new)
  .content
    %h2.title
      =pat(:new)
      =mt(:streamhub)
    .inner
      -form_for :streamhub, url(:streamhubs, :create), :class => :form do |f|
        =partial "streamhubs/form", :locals => { :f => f }

-content_for :sidebar, partial("base/sidebar")
